# Allgemeine Konfiguration
WORKSPACE_PATH=/workspace
COMPOSE_PROJECT_NAME=mcp-ecosystem

# Redis
REDIS_PASSWORD=redis_password

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=mcp

# GitHub
GITHUB_TOKEN=your_github_token

# GitLab
GITLAB_TOKEN=your_gitlab_token

# Wolfram Alpha
WOLFRAM_ALPHA_APPID=your_wolfram_alpha_appid

# Discord
DISCORD_TOKEN=your_discord_token

# n8n
N8N_ENCRYPTION_KEY=your_n8n_encryption_key

# OpenProject
OPENPROJECT_SECRET_KEY_BASE=your_openproject_secret_key_base

# Display f√ºr Desktop Commander
DISPLAY=:0

# Ollama
OLLAMA_MODELS=qwen2.5-coder:7b-instruct,llama3:8b,mistral:7b-instruct-v0.2

# Sicherheit
ENABLE_TELEMETRY=false
ALLOW_REMOTE_CODE_EXECUTION=false
ALLOW_SHELL_COMMANDS=false

# Logging
LOG_LEVEL=info
LOG_FORMAT=json